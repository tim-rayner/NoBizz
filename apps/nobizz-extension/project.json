{
  "name": "nobizz-extension",
  "sourceRoot": "apps/nobizz-extension/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "plasmo dev",
        "cwd": "{projectRoot}"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "plasmo build",
        "cwd": "{projectRoot}"
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "plasmo package",
        "cwd": "{projectRoot}"
      }
    }
  }
}
